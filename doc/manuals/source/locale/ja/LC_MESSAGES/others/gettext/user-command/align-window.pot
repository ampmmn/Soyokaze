msgid "押下キー"
msgstr ""

#: ../../source/user-command/align-window.md:44
msgid "動作"
msgstr ""

#: ../../source/user-command/align-window.md:44
msgid "`Enter`"
msgstr ""

#: ../../source/user-command/align-window.md:44
msgid "ウインドウの整列を実行する"
msgstr ""
